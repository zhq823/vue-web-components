  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zhq823/vue-web-components/issues"
  },
  "homepage": "https://github.com/zhq823/vue-web-components#readme",
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
